{
    "esm": {
        "version": "2.5",
        "website": "http://www.ezservermonitor.com",
        "check_updates": true,
        "auto_refresh": 10,
        "theme": "dark",
        "custom_title": "CMSC828T Dashboard"
    },
    "cpu": {
        "enable_temperature": true
    },
    "disk": {
        "show_tmpfs": false,
        "show_filesystem": true
    },
    "ping": {
        "hosts": [
            "cmsc828t.cs.umd.edu",
            "google.com",
            "yahoo.com"
        ]
    },
    "last_login": {
        "enable": true,
        "max": 5
    },
    "services": {
        "show_port": true,
        "list": [
            {
                "name": "Web Server",
                "host": "localhost",
                "port": 80,
                "protocol": "tcp"
            },
            {
                "name": "HTTPS",
                "host": "localhost",
                "port": 443,
                "protocol": "tcp"
            },
            {
                "name": "Database Server",
                "host": "localhost",
                "port": 3306,
                "protocol": "tcp"
            },
            {
                "name": "SSH",
                "host": "localhost",
                "port": 22,
                "protocol": "tcp"
            }
        ]
    }
}
